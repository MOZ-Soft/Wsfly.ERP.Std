<mz:BaseUserControl x:Class="Wsfly.ERP.Std.Views.Parts.PlugInsUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Wsfly.ERP.Std.Views.Parts"
             xmlns:mz="clr-namespace:Wsfly.ERP.Std.AppCode.Base"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="600">
    <Grid x:Name="gridMain">
        <TabControl x:Name="tabMain">
            <TabItem Header="所有插件">
                <StackPanel x:Name="panelAllPlugIns">
                    
                </StackPanel>
            </TabItem>
            <TabItem Header="已安装插件">
                <StackPanel x:Name="panelInstallPlugIns">
                </StackPanel>
            </TabItem>
        </TabControl>
    </Grid>
</mz:BaseUserControl>
